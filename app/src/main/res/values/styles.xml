        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="redeemcalender"> 
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>
    <style name="save_btn"> 
        <item name="android:textSize">@dimen/font14</item>
        <item name="android:background">@color/light_green</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="redeem_btn"> 
        <item name="android:textSize">@dimen/font14</item>
        <item name="android:background">@color/redeem_btn</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:textColor">@color/white</item>
    </style>
     <style name="calender_icon">
        <item name="android:layout_width">30dp</item>  
          <item name="android:layout_height">30dp</item>  
          <item name="android:layout_marginLeft">10dp</item>
    </style>
    <style name="setting_icon">
        <item name="android:layout_width">15dp</item>  
          <item name="android:layout_height">15dp</item>  
    </style>
	<style name="pushNotification">
         <item name="android:textSize">@dimen/font18</item>
         <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="white_bg">
        <item name="android:background">@color/white</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
     <style name="geoTabtxt"> 
         <item name="android:textSize">@dimen/font12</item>
    </style>
    <style name="redeemed_banner"> 
      <item name="android:layout_width">200dp</item>  
          <item name="android:layout_height">75dp</item>  
    </style>
     <style name="white_bg_banner">
        <item name="android:background">@color/white</item>
        <item name="android:padding">10dp</item>
    </style>
    <style name="PageBg">
        <item name="android:background">#fff</item>
    </style>
    <style name="navtopwrapper">
        <item name="android:padding">5dp</item>
    </style>
    <style name="navlogo">
        <item name="android:layout_marginTop">5dp</item>
    </style>
</resources>
