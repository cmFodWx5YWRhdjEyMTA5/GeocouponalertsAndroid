<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="border_gray">
        <item name="android:background">#d7d7d7</item>
        <item name="android:layout_marginTop">10dp</item>
       
    </style>
    
    <style name="activationCode">
        <item name="android:textSize">@dimen/activationCode</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#eee</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:layout_height">35dp</item>
    </style>
    
    <style name="logo">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    
     <style name="business_logo">
        <item name="android:layout_marginRight">5dp</item>  
        <item name="android:layout_height">90dp</item>  
        <item name="android:layout_width">0dp</item>      
    </style>
    <style name="business_logo1">
        <item name="android:layout_marginRight">5dp</item>  
        <item name="android:layout_height">90dp</item>  
        <item name="android:layout_width">90dp</item>      
    </style>
      <style name="border">
        <item name="android:background">#8f898f</item>
        <item name="android:layout_marginTop">5dp</item>
       
    </style>
    <style name="border2">
        <item name="android:background">#8f898f</item>
              
    </style>
    <style name="border1">
        <item name="android:background">#000000</item>
        <item name="android:layout_marginTop">1dp</item>
       
    </style>
    
      <style name="check_business">
          <item name="android:layout_width">30dp</item>  
          <item name="android:layout_height">30dp</item>  
         
    </style>
    
      <style name="table_layout_height">
           <item name="android:layout_height">90dp</item>  
    </style>
    
      <style name="actNav">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="bold_16">
         <item name="android:textSize">@dimen/font16</item> 
          <item name="android:textStyle">bold</item>  
    </style>
    <style name="green_bg_banner">
        <item name="android:background">@color/light_green</item>
    </style>
     <style name="green_bg">
        <item name="android:background">@color/light_green</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="back_arrow">
        <item name="android:layout_width">30dp</item>  
          <item name="android:layout_height">30dp</item>  
    </style>
    <style name="greay_bg">
        <item name="android:background">#ebebeb</item>
        <item name="android:paddingBottom">25dp</item>
    </style>
    <style name="white_bg_list">
        <item name="android:background">@color/white</item>
        <item name="android:layout_margin">10dp</item>
    </style>
     <style name="tag_right">
        <item name="android:layout_width">80dp</item>  
          <item name="android:layout_height">80dp</item>  
    </style>
    <style name="starBucksTitle"> 
         <item name="android:textSize">@dimen/font16</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="starBuckstxt"> 
        <item name="android:textSize">@dimen/font14</item>
        <item name="android:textColor">@color/black</item>
         <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="starBucksorg"> 
        <item name="android:textSize">@dimen/font14</item>
        <item name="android:textColor">@color/orange</item>
         <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>
    <style name="otherOffer"> 
        <item name="android:textSize">@dimen/font14</item>
        <item name="android:textColor">@color/black</item>
